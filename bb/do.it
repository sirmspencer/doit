task1:
  clojure -M:project/1

task2:
  clojure -M:project/2

task3 ^override:
  clojure -M:project/3

task4: ^override:
  clojure -M:project/4

params1:
  --run home

alias1:
  :env/home

ls:
  ls -lash

cp-brew:
  cp /usr/local/Homebrew/Library/Taps/homebrew/homebrew-core/Formula/d/doit.rb ~/ext-github/tap/doit.rb

auto-commit:
  git add .
  && git commit -m auto-publish

publish:
  ^auto-commit
  && git push
  && sh auto-increment.sh
  && git push --tags
