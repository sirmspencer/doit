re:
  brew reinstall doit -s

edit:
  open /usr/local/Homebrew/Library/Taps/homebrew/homebrew-core/Formula/d/doit.rb

cp-brew:
  cp /usr/local/Homebrew/Library/Taps/homebrew/homebrew-core/Formula/d/doit.rb ~/ext-github/tap/doit.rb

lls:
  ls -lash

auto-commit:
  git add .
  && git commit -m auto-publish

publish:
  ^auto-commit
  && git push
  && sh auto-increment.sh
  && git push --tags
